{
  "name": "oss",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "preinstall": "npm run doctor",
    "next-hello:build": "nx build next-hello",
    "next-hello:dev": "nx serve next-hello",
    "next-hello:test": "nx test next-hello",
    "next-hello:typecheck": "nx typecheck next-hello",
    "ayokoding-web:build": "cd apps-standalone/ayokoding-web && npm run build",
    "ayokoding-web:dev": "cd apps-standalone/ayokoding-web && npm run dev",
    "analisapasar-web:build": "cd apps-standalone/analisapasar-web && npm run build",
    "analisapasar-web:dev": "cd apps-standalone/analisapasar-web && npm run dev",
    "wahidyankf-web:build": "cd apps-standalone/wahidyankf-web && npm run build",
    "wahidyankf-web:dev": "cd apps-standalone/wahidyankf-web && npm run dev",
    "test:echo": "echo 'Running tests for Ayokoding monorepo'",
    "typecheck": "npx nx run-many --target=typecheck",
    "test:all:standalone": "npm run build:standalone",
    "test:all": "npm run test:echo && npm run test:all:standalone && npx nx run-many --target=test && npx nx run-many --target=typecheck && npx nx run-many --target=build",
    "build": "./scripts/build.sh",
    "build:standalone": "npm run ayokoding-web:build && npm run analisapasar-web:build && npm run wahidyankf-web:build",
    "doctor": "node scripts/doctor.js"
  },
  "devDependencies": {
    "@babel/core": "^7.26.8",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/plugin-transform-runtime": "^7.26.8",
    "@babel/preset-env": "^7.26.8",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@nx/eslint": "^20.4.2",
    "@nx/jest": "^20.4.2",
    "@nx/js": "^20.4.2",
    "@nx/next": "^20.4.2",
    "@nx/react": "^20.4.2",
    "@tailwindcss/postcss": "^4.0.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "babel-jest": "^29.7.0",
    "husky": "^9.1.7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.4.3",
    "nx": "^20.4.2",
    "postcss": "^8.5.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^4.0.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.2"
  },
  "dependencies": {
    "@nx/web": "^20.4.2",
    "next": "^15.1.6"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,md,mdx,yaml,mjs,json}": [
      "prettier --write"
    ],
    "*.py": [
      "black --quiet"
    ]
  },
  "volta": {
    "node": "20.17.0",
    "npm": "11.1.0"
  }
}
